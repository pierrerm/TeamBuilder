<core:FragmentDefinition xmlns:form="sap.ui.layout.form" xmlns:core="sap.ui.core" xmlns="sap.m"
	xmlns:uxap="sap.uxap" xmlns:viz="sap.viz.ui5.controls" xmlns:layout="sap.ui.layout"
    xmlns:mvc="sap.ui.core.mvc" xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds" xmlns:viz.data="sap.viz.ui5.data" height="100%">
	<Dialog id="viewCompetencesDialog" title="View Competences" stretch="false" xmlns:f="sap.f" contentHeight="175px" contentWidth="500px">
		<layout:Grid defaultSpan="L12 M12 S12" width="500px">
			<layout:content>
				<viz:VizFrame id="competenceChart" vizType="column" width="100%" height="100%">
				</viz:VizFrame>
			</layout:content>
		</layout:Grid>
		<buttons>
			<Button text="Close" press="onCloseViewCompetences"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>